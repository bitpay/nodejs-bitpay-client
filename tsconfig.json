{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "declaration": true,
    "target": "es5",
    "outDir": "./dist",
    "sourceMap": true,
    "strictNullChecks": false,
    "strictPropertyInitialization": false
  },
  "lib": [
    "es5",
    "node"
  ],
  "include": [
    "src/**/*.ts",
    "setup/*.ts"
  ],
  "exclude": [
    "src/**/*.spec.ts"
  ]
}
